{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "airbnb" 
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "import/no-extraneous-dependencies": "off",
        "react/destructuring-assignment": "off",
        "react/no-access-state-in-setstate": "off",
        "react/button-has-type": "off"

    },
    "settings":{
        "react":{
            "version":"latest"
        }
    }
    
}
